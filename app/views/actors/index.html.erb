<h1>Actors</h1>
<ul>
<% @actors.each do |actor| %>
<img src="<%=actor.image_url%>" alt="">
<h1><%=actor.name%></h1>
<p><%=actor.bio%></p>
<p><%= actor.birth_date %></p>
<p><%= actor.birth_place %></p>
<% if(actor.alive != true) %>
<p>Deat Date: <%= actor.death_date %></p>
<p>Deat Place: <%= actor.death_place %></p>
<% end %>
<% end %>
</ul>

<h2><a href="/actors/new">Crear Actor</a></h2>
