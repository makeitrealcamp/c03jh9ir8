<h1>Actores</h1>

<%@actors.each do |a|%>

    <ul>
      <li> <%=a.name%> </li>
      <li> <%=a.bio%> </li>
      <li> <%=a.birth_date%> </li>
      <li> <%=a.birth_place%> </li>
      <li> <%=a.image_url%> </li>
      <li> <%=a.alive%> </li>
      <li> <%=a.death_date%> </li>
      <li> <%=a.death_place%> </li>
    </ul>
<%end%>



<%= link_to "Crear Actor", new_actor_path %>
