<%= form_for :actor, url: actors_path do |f| %>
<%=f.label :name %><br>
<%=f.text_field :name%><br>
<%=f.label :bio %><br>
<%=f.text_field :bio%><br>
<%=f.label :birth_date %><br>
<%=f.date_field :birth_date%><br>
<%=f.label :birth_place %><br>
<%=f.text_field :birth_place%><br>
<%=f.label :image_url %><br>
<%=f.text_field :image_url%><br>
<%=f.label :alive %><br>
<%=f.text_field :alive%><br>
<%=f.label :death_date %><br>
<%=f.date_field :death_date%><br>
<%=f.label :death_place %><br>
<%=f.text_field :death_place%><br>
<%=f.submit %>
<%end%>