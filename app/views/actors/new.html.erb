
<%= form_for @actor do |f| %>
 <div>
    <%= f.label :image_url %><br>
    <%= f.text_field :image_url %>
  </div>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :bio %><br>
    <%= f.text_field :bio %>
  </div>
   <div>
    <%= f.label :birth_date %><br>
    <%= f.text_field :birth_date %>
  </div>
   <div>
    <%= f.label :birth_place %><br>
    <%= f.text_field :birth_place %>
  </div>
   <div>
    <%= f.label :death_date %><br>
    <%= f.text_field :death_date %>
  </div>
   <div>
    <div>
    <%= f.label :death_place %><br>
    <%= f.text_field :death_place %>
  </div>
 <div>
    <%= f.submit %>
  </div>
<% end %>