 <%= form_for @actor, url: actors_path, method: :post do |f| %>
  
  <div>
    <%= f.label :actor_makeitreal %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :bio_makeitreal %>
    <%= f.text_field :bio %>
  </div>
  <div>
    <%= f.label :birth_date_makeitreal %>
    <%= f.date_field :birth_date %>
  </div> 
  <div>
    <%= f.label :birth_place_makeitreal %>
    <%= f.text_field :birth_place %>
  </div>
  <div>
    <%= f.submit %>
  </div>

<% end %>
