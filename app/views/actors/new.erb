<%= form_for :actor, url: actors_path do |f| %>
  <p>
    <%= f.label :foto %><br>
    <%= f.text_area :image_url %>
  </p>
 
  <p>
    <%= f.label :nombre %><br>
    <%= f.text_area :name %>
  </p>
 


  <p>
    <%= f.label :bio%><br>
    <%= f.text_area :bio %>
  </p>

 <p>
    <%= f.label :fecha_nacimiento %><br>
    <%= f.text_area :birth_date %>
  </p>
 
  <p>
    <%= f.label :Lugar_nacimiento %><br>
    <%= f.text_area :birth_place %>
  </p>

    <p>
    <%= f.label :fecha_fallecimiento %><br>
    <%= f.text_area :death_date %>
  </p>

      <p>
    <%= f.label :lugar_facellimiento %><br>
    <%= f.text_area :death_place %>
  </p>








  <p>
    <%= f.submit %>
  </p>
<% end %>